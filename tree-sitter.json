{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "alv",
      "camelcase": "Alv",
      "title": "alv",
      "scope": "source.alv",
      "file-types": [
        "alv"
      ],
      "injection-regex": "^alv$",
      "class-name": "TreeSitterAlv"
    }
  ],
  "metadata": {
    "version": "0.3.0",
    "license": "MIT",
    "description": "the alv livecoding language",
    "authors": [
      {
        "name": "s-ol"
      }
    ],
    "links": {
      "repository": "https://github.com/s-ol/tree-sitter-alv"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}